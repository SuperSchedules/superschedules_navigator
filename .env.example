# Superschedules Navigator Environment Variables
# Copy this to .env and fill in your values

# API Configuration (required for pushing discoveries)
SUPERSCHEDULES_API_URL=https://api.eventzombie.com
SUPERSCHEDULES_API_TOKEN=your-api-token-here

# Database (optional - defaults work for local PostgreSQL)
# DB_NAME=navigator
# DB_USER=
# DB_PASSWORD=
# DB_HOST=localhost
# DB_PORT=5432

# Vision Model (optional)
# VISION_MODEL=minicpm-v
# OLLAMA_URL=http://localhost:11434

# Django (optional)
# DJANGO_SECRET_KEY=your-secret-key-here
# DEBUG=True
# ALLOWED_HOSTS=localhost,127.0.0.1
